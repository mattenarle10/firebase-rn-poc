# Copy this file to .env and fill in values from your Firebase Console (Project Settings > General > Your apps > Web app config)
EXPO_PUBLIC_FIREBASE_API_KEY=
EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN=
EXPO_PUBLIC_FIREBASE_PROJECT_ID=
EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET=
EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=
EXPO_PUBLIC_FIREBASE_APP_ID=
# Optional (for web analytics)
EXPO_PUBLIC_FIREBASE_MEASUREMENT_ID=

# Google OAuth Client IDs (from Google Cloud Console)
# For Expo Go and development (Web client type in Google Cloud)
EXPO_PUBLIC_GOOGLE_EXPO_CLIENT_ID=
# For standalone iOS app (iOS client type in Google Cloud)
EXPO_PUBLIC_GOOGLE_IOS_CLIENT_ID=
# For standalone Android app (Android client type in Google Cloud)
EXPO_PUBLIC_GOOGLE_ANDROID_CLIENT_ID=

